{
	"name": "dunkel/graphqlsites",
	"description": "Add a graphql query for sites to craft cms.",
	"type": "craft-plugin",
	"license": "MIT",
	"support": {
		"source": "https://github.com/Dunkel-Design/craft-graphql-sites"
	},
	"authors": [
		{
			"name": "Dunkel Design",
			"homepage": "https://dunkel.cc"
		}
	],
	"require": {
		"php": ">=8.0.2",
		"craftcms/cms": "^4.4.8 || ^5.0.0-beta.1"
	},
	"autoload": {
		"psr-4": {
			"dunkel\\graphqlsites\\": "src/"
		}
	},
	"extra": {
		"handle": "_craft-graphql-sites",
		"name": "GraphQL Sites",
		"developer": "Dunkel",
		"documentationUrl": "",
		"class": "dunkel\\graphqlsites\\GraphqlSites"
	},
	"config": {
		"sort-packages": true,
		"platform": {
			"php": "8.0.2"
		},
		"allow-plugins": {
			"yiisoft/yii2-composer": true,
			"craftcms/plugin-installer": true
		}
	}
}

